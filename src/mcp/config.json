{
  "mcpServers": {
    "qasioun-marketplace": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres"],
      "env": {
        "POSTGRES_CONNECTION_STRING": "${MCP_DATABASE_URL}"
      },
      "description": "Qasioun Marketplace PostgreSQL database for listings, categories, and attributes"
    }
  },
  "settings": {
    "defaultServer": "qasioun-marketplace",
    "timeout": 30000,
    "maxRetries": 3
  }
}
